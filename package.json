{
  "name": "take-home-backend",
  "type": "module",
  "version": "1.0.0",
  "description": "Take-home exercise: Import Pipeline with Hono + Supabase",
  "main": "dist/server.js",
  "scripts": {
    "dev": "dotenv -- tsx watch src/server.ts",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "graphile-worker:install": "dotenv -- npx --node-options='--import tsx/esm' graphile-worker --schema-only",
    "graphile-worker:run-once": "dotenv -- npx --node-options='--import tsx/esm' graphile-worker --once",
    "worker": "dotenv -- tsx src/workers/worker.ts",
    "e2e": "./scripts/e2e-test.sh",
    "fix": "biome check --write ."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "1.18.1",
    "@hono/zod-validator": "^0.7.2",
    "@supabase/supabase-js": "2.54.0",
    "graphile-worker": "0.16.6",
    "hono": "4.9.0",
    "tus-js-client": "^4.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@types/node": "20.10.5",
    "dotenv-cli": "^8.0.0",
    "supabase": "2.33.9",
    "ts-node": "^10.9.2",
    "tsx": "4.20.3",
    "typescript": "5.9.2"
  }
}
